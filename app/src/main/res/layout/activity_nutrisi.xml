<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Nutrisi">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/beranda1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/transparan"
            android:paddingLeft="10dp"
            android:paddingTop="11dp"
            android:src="@drawable/home_black"
            tools:ignore="SpeakableTextPresentCheck" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutrisi"
            android:textColor="@color/primaryColor"
            android:paddingTop="15dp"
            android:paddingLeft="5dp"
            android:fontFamily="@font/poppinsmedium"
            android:textStyle="bold"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="20dp"
        tools:ignore="MissingConstraints">

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="45dp"
            android:width="350dp"
            android:background="@drawable/pencarian_beranda"
            android:drawableLeft="@drawable/pencarian"
            android:paddingLeft="10dp"
            android:paddingTop="10dp"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingTop="25dp"
        android:layout_marginTop="105dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="15dp">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="129dp"
                android:layout_margin="8dp"
                android:layout_marginBottom="5dp"
                android:outlineSpotShadowColor="@color/primaryColor"
                app:cardCornerRadius="15dp"
                app:cardElevation="9dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageButton
                        android:id="@+id/imgbtn"
                        android:layout_width="132dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="16dp"
                        android:background="@color/transparan"
                        android:src="@drawable/image_2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text"
                        android:layout_width="90dp"
                        android:layout_height="25dp"
                        android:background="@drawable/kotak"
                        android:fontFamily="@font/oswald_regular"
                        android:text="Nutrisi remaja"
                        android:textColor="@color/primaryColor"
                       android:layout_toRightOf="@+id/imgbtn"
                        android:layout_marginTop="25dp"
                        android:gravity="center"/>

                    <TextView
                        android:id="@+id/text2"
                        android:layout_width="90dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="108dp"
                        android:layout_marginTop="25dp"
                        android:layout_toRightOf="@+id/imgbtn"
                        android:background="@drawable/kotak"
                        android:fontFamily="@font/oswald_regular"
                        android:gravity="center"
                        android:text="Nutrisi"
                        android:textColor="@color/primaryColor" />

                    <TextView
                        android:id="@+id/text1"
                        android:layout_width="247dp"
                        android:layout_height="72dp"
                        android:layout_below="@+id/text"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="3dp"
                        android:layout_toRightOf="@id/imgbtn"
                        android:fontFamily="@font/oswald_medium"
                        android:text="Pengaruh Pemberian Nutrisi Pada Remaja dan Dapaknya Bagi Pertumbuhan"
                        android:textSize="15dp"
                        android:textColor="@color/primaryColor" />


                </RelativeLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="129dp"
                android:layout_margin="8dp"
                android:layout_marginBottom="5dp"
                android:outlineSpotShadowColor="@color/primaryColor"
                app:cardCornerRadius="15dp"
                app:cardElevation="9dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageButton
                        android:id="@+id/imgbtn2"
                        android:layout_width="132dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="16dp"
                        android:background="@color/transparan"
                        android:src="@drawable/image_2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text3"
                        android:layout_width="90dp"
                        android:layout_height="25dp"
                        android:background="@drawable/kotak"
                        android:fontFamily="@font/oswald_regular"
                        android:text="Hidup Sehat"
                        android:textColor="@color/primaryColor"
                        android:layout_toRightOf="@+id/imgbtn2"
                        android:layout_marginTop="25dp"
                        android:gravity="center"/>



                    <TextView
                        android:id="@+id/text4"
                        android:layout_width="247dp"
                        android:layout_height="72dp"
                        android:layout_below="@+id/text3"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="3dp"
                        android:layout_toRightOf="@id/imgbtn2"
                        android:fontFamily="@font/oswald_medium"
                        android:text="NUTRISI: PENGERTIAN, FUNGSI, DAN JENIS-JENISNYA"
                        android:textSize="15dp"
                        android:textColor="@color/primaryColor" />


                </RelativeLayout>

            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="129dp"
                android:layout_margin="8dp"
                android:layout_marginBottom="5dp"
                android:outlineSpotShadowColor="@color/primaryColor"
                app:cardCornerRadius="15dp"
                app:cardElevation="9dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageButton
                        android:id="@+id/imgbtn3"
                        android:layout_width="132dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="16dp"
                        android:background="@color/transparan"
                        android:src="@drawable/image_2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text6"
                        android:layout_width="130dp"
                        android:layout_height="25dp"
                        android:background="@drawable/kotak"
                        android:fontFamily="@font/oswald_regular"
                        android:text="Nutrisi dan Kesehatan"
                        android:textColor="@color/primaryColor"
                        android:layout_toRightOf="@+id/imgbtn3"
                        android:layout_marginTop="25dp"
                        android:gravity="center"/>



                    <TextView
                        android:id="@+id/text5"
                        android:layout_width="247dp"
                        android:layout_height="72dp"
                        android:layout_below="@+id/text6"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="3dp"
                        android:layout_toRightOf="@id/imgbtn3"
                        android:fontFamily="@font/oswald_medium"
                        android:text="5 Jenis Nutrisi yang Harus Dipenuhi untuk Tubuh Sehat"
                        android:textSize="15dp"
                        android:textColor="@color/primaryColor" />


                </RelativeLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="129dp"
                android:layout_margin="8dp"
                android:layout_marginBottom="5dp"
                android:outlineSpotShadowColor="@color/primaryColor"
                app:cardCornerRadius="15dp"
                app:cardElevation="9dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageButton
                        android:id="@+id/imgbtn4"
                        android:layout_width="132dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="16dp"
                        android:background="@color/transparan"
                        android:src="@drawable/image_2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text7"
                        android:layout_width="130dp"
                        android:layout_height="25dp"
                        android:background="@drawable/kotak"
                        android:fontFamily="@font/oswald_regular"
                        android:text="Teknologi dan Bisnis"
                        android:textColor="@color/primaryColor"
                        android:layout_toRightOf="@+id/imgbtn4"
                        android:layout_marginTop="25dp"
                        android:gravity="center"/>



                    <TextView
                        android:id="@+id/text8"
                        android:layout_width="247dp"
                        android:layout_height="72dp"
                        android:layout_below="@+id/text7"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="3dp"
                        android:layout_toRightOf="@id/imgbtn4"
                        android:fontFamily="@font/oswald_medium"
                        android:text="Apa Itu Nutrisi: Pengertian dan Manfaat Nutrisi Untuk Tubuh"
                        android:textSize="15dp"
                        android:textColor="@color/primaryColor" />


                </RelativeLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="129dp"
                android:layout_margin="8dp"
                android:layout_marginBottom="5dp"
                android:outlineSpotShadowColor="@color/primaryColor"
                app:cardCornerRadius="15dp"
                app:cardElevation="9dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageButton
                        android:id="@+id/imgbtn5"
                        android:layout_width="132dp"
                        android:layout_height="150dp"
                        android:layout_marginStart="16dp"
                        android:background="@color/transparan"
                        android:src="@drawable/image_2"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text9"
                        android:layout_width="130dp"
                        android:layout_height="25dp"
                        android:background="@drawable/kotak"
                        android:fontFamily="@font/oswald_regular"
                        android:text="Stories"
                        android:textColor="@color/primaryColor"
                        android:layout_toRightOf="@+id/imgbtn5"
                        android:layout_marginTop="25dp"
                        android:gravity="center"/>



                    <TextView
                        android:id="@+id/text10"
                        android:layout_width="247dp"
                        android:layout_height="72dp"
                        android:layout_below="@+id/text9"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="3dp"
                        android:layout_toRightOf="@id/imgbtn5"
                        android:fontFamily="@font/oswald_medium"
                        android:text="7 Nutrisi Penting Yang Diperlukan Tubuh Setiap Hari"
                        android:textSize="15dp"
                        android:textColor="@color/primaryColor" />


                </RelativeLayout>

            </androidx.cardview.widget.CardView>



        </LinearLayout>


    </ScrollView>



</RelativeLayout>